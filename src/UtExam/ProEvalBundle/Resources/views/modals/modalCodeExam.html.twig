{% extends 'base.html.twig' %}
{% block main %}
  <div class="modal fade" id="ModalCodeExam" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Codigo del Examen</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form action="examenFijo" method="GET">
            <div class="form-group">
              <label for="recipient-name" class="col-form-label">Ingresa el codigo del examen:</label>
              <input type="text" class="form-control lblCodeExam" name="codeExam" id="recipient-name" required>
            </div>
            <input type="submit" class="btn btn-primary btnSuccesCodeExam" name="" value="Aceptar">
          </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
        </div>
      </div>
    </div>
  </div>
{% endblock %}
